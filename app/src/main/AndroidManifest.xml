<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_utfpr"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_utfpr_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ThemeStylesIcons"
        tools:targetApi="31">
        <activity
            android:name=".PessoasActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PessoaActivity"
            android:exported="false">
            <!-- não faz sentido
            android:exported ser true quando não há intent-filter
            se removermos esse atributo o seu valor padão é false
            -->
        </activity>
    </application>

</manifest>